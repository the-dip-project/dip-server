{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/dip.server/src",
  "monorepo": true,
  "root": "apps/dip.server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/dip.server/tsconfig.app.json"
  },
  "projects": {
    "dip.server": {
      "type": "application",
      "root": "apps/dip.server",
      "entryFile": "main",
      "sourceRoot": "apps/dip.server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dip.server/tsconfig.app.json"
      }
    },
    "view": {
      "type": "application",
      "root": "apps/view",
      "entryFile": "index",
      "sourceRoot": "apps/view/src",
      "compilerOptions": {
        "webpack": true,
        "tsConfigPath": "apps/view/tsconfig.app.json",
        "webpackConfigPath": "apps/view/webpack.config.js"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}